<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>phonenumbervalidationrule</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(REGEX(Phone__c, &quot;\\D*?(\\d\\D*?){10}&quot;))</errorConditionFormula>
    <errorDisplayField>Phone__c</errorDisplayField>
    <errorMessage>Please enter a valid phone number</errorMessage>
</ValidationRule>
